<div class="place-box">
<div class="place" data-toggle="collapse" data-target="#demo-place1">
  <span class="blurb">
    <h1>{{place.name}}</h1>
    <p>{{place.description}}</p>
  </span>
  <div class="place-detail" id="demo-place1">
    <div class="actions">
      <h3>More Info</h3>
      <ul>
        <li>{{place.place_type.name}}</li>
        {{#each place.place_subtypes}}
        	<li>{{name}}</li>
        {{/each}}
        {{#if place.website}}
        	<li>{{place.website}}</li>
        {{/if}}
      </ul>
      {{#each place.reviews}}
      <span>
        {{author_name}}
        {{text}}
      </span>
      {{/each}}
      <button class="action-example">Send to my Email</button>
      <button class="utility-example">Share on Facebook</button>
    </div>
    <div id="detail-map-canvas">
    </div>
  </div>
</div>
</div>

<script type="text/javascript">
	App.InitMapDetail();
</script>

